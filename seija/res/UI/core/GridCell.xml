<GridCell>
    <Template>
        <Entity>
            <Components>
                <Transform />
                <Rect2D />
                <GridCell row="(attr :row)" col="(attr :col)" rowSpan="(attr :rowSpan)" colSpan="(attr :colSpan)" />
                <ContentView size="(vec2 (attr :width) (attr :height))"
                             hor = "(attr :hor)"
                             ver = "(attr :ver)"
                             margin="(attr :margin)"
                             padding="(attr :padding)"
                             position="(attr :position)" />
            </Components>
            <Slot.Children />
        </Entity>
    </Template>
</GridCell>