<Panel xmlns:core="/core/">
    <Template>
        <Entity>
            <Components>
                <Transform />
                <Rect2D />
                <ContentView size="(vec2 (attr :width) (attr :height))"
                             hor = "(attr :hor)"
                             ver = "(attr :ver)"
                             margin="(attr :margin)"
                             padding="(attr :padding)"
                             position="(attr :position)"
                             viewType="(attr :viewType)" />
            </Components>
            <Slot.Children />
        </Entity>
    </Template>
</Panel>