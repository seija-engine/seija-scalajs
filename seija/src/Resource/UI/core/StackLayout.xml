<StackLayout>
    <Template>
        <Entity>
            <Components>
                <Transform position="0,0,-10" />
                <Rect2D />
                <StackLayout margin="(attr-bind :margin)"
                             padding="(attr-bind :padding)"
                             hor="(attr-bind :hor)"
                             ver="(attr-bind :ver)"
                             orientation="(attr-bind :orientation)"
                />
                <ImageRender texture="(env res.white)" color="#999999" />
                <Transparent />
            </Components>
            <Slot.Children />
        </Entity>
    </Template>
</StackLayout>
